<view class="page">
  <view class="card">
    <view class="h2">我的收藏</view>
  </view>
  <scroll-view scroll-y style="height: calc(100vh - 240rpx);">
    <block wx:for="{{list}}" wx:key="id">
      <view class="card" bindtap="openDetail" data-id="{{item.id}}">
        <view class="row space-between">
          <view>{{item.title}}</view>
          <image class="list-star" src="/images/icons/favorite-on.svg" mode="aspectFit"></image>
        </view>
      </view>
    </block>
  </scroll-view>
</view>